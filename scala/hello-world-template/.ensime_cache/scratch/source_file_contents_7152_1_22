import math._
import util.Random
object Main extends App {
  println("Hello, World!")

  def something(n: Int) = {
    val x = new Array[Int](n)
    for(i <- 0 until x.length) {
      x(i) = Random.nextInt(n)
    }
    x
  }

  def otherthing(a: Array[Int]): Array[Int] = {
    for(i <- a) yield {
      vaj = a.indexOf(i)

    }
  }
}

